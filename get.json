// Make a GET request to your Roblox server with the datastore key
fetch("https://www.roblox.com/datastore/get-json?assetId=2332891023&key=Dataset")
  .then(response => response.json()) // Parse the JSON response
  .then(data => {
    // Use the data to update the HTML on your site
    const resultElement = document.getElementById("result");
    resultElement.innerHTML = `Datastore value: ${data}`;
  })
  .catch(error => {
    console.error(error);
  });
